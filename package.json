{
  "name": "litetics",
  "version": "1.0.0-rc.1",
  "description": "Embeddable javascript analytics event tracking library",
  "repository": "Hrdtr/litetics",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./handler": {
      "types": "./dist/handler/index.d.ts",
      "import": "./dist/handler/index.mjs",
      "require": "./dist/handler/index.cjs"
    },
    "./tracker": {
      "types": "./dist/tracker/index.d.ts",
      "import": "./dist/tracker/index.mjs",
      "require": "./dist/tracker/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev --dom",
    "lint": "eslint .",
    "lint:fix": "automd && eslint . --fix",
    "prepack": "pnpm build",
    "prepare": "husky && echo \"pnpm lint-staged\" > .husky/pre-commit && echo \"pnpm commitlint --edit ${1}\" > .husky/commit-msg",
    "play": "jiti playground",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "release:rc": "pnpm test && changelogen --release --prerelease rc && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm typecheck && vitest run --dom",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@types/node": "^20.14.9",
    "@types/ua-parser-js": "^0.7.39",
    "@vitest/coverage-v8": "^1.5.3",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "commitlint": "^19.3.0",
    "consola": "^3.2.3",
    "eslint": "^9.6.0",
    "eslint-config-unjs": "^0.3.2",
    "happy-dom": "^14.12.3",
    "husky": "^9.0.11",
    "isbot": "^5.1.13",
    "jiti": "^1.21.6",
    "lint-staged": "^15.2.7",
    "typescript": "^5.5.3",
    "ua-parser-js": "^1.0.38",
    "unbuild": "^2.0.0",
    "vitepress": "^1.2.3",
    "vitest": "^1.5.3"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "isbot": "^5.1.13",
    "ua-parser-js": "^1.0.38"
  },
  "packageManager": "pnpm@9.2.0",
  "lint-staged": {
    "**/*.{ts,js,mjs,vue,html,json,yml}": [
      "pnpm lint:fix"
    ]
  }
}
